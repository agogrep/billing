<?xml version="1.0"?>
<script>
    <part>
      <section>
        <parentselector>.//*[@id="contracts"]</parentselector>
        <selector>.//*[@name="row"]</selector>
        <repeat>0</repeat>
      </section>
      <sql>
        <readquery></readquery>
        <writequery></writequery>
        <readgenerator>
          <scriptname>journal</scriptname>
          <param>
            <table>contracts</table>
            <id_name>cid</id_name>
            <countrows>10</countrows>
          </param>
        </readgenerator>
        <argument>
          <cid><type>int</type><long></long><default></default></cid>
          <descr><type>str</type><long></long><default></default></descr>
          <cdate><type>str</type><long></long><default></default></cdate>
          <enddate><type>str</type><long></long><default></default></enddate>
        </argument>
      </sql>
      <layout>
          <element><selector>.//*[@name="cid"]</selector><attribute></attribute><fild>cid</fild></element>
          <element><selector>.//*[@name="cid"]</selector><attribute>data-href</attribute><fild>cid</fild></element>
          <element><selector>.//*[@name="descr"]</selector><attribute></attribute><fild>descr</fild></element>
          <element><selector>.//*[@name="cdate"]</selector><attribute></attribute><fild>cdate</fild></element>
          <element><selector>.//*[@name="enddate"]</selector><attribute></attribute><fild>enddate</fild></element>
      </layout>
    </part>
    <part></part>
  </script>
