<?xml version="1.0"?>
<script>

    <part>
      <name>journal</name>
      <section>
        <parentselector>.//*[@id="accounts"]</parentselector>
        <selector>.//*[@name="row"]</selector>
        <repeat>0</repeat>
      </section>
      <sql>
        <readquery></readquery>
        <writequery></writequery>
        <readgenerator>
          <scriptname>journal_accounts</scriptname>
          <param>
            <table>accounts</table>
            <id_name>aid</id_name>
            <countrows>20</countrows>
          </param>
        </readgenerator>
        <argument>
          <aid>
            <type>int</type>
            <long></long>
            <default></default>
          </aid>
          <aname>
            <type>str</type>
            <long></long>
            <default></default>
          </aname>
          <curr>
            <type>str</type>
            <long></long>
            <default></default>
          </curr>
          <apdate>
            <type>str</type>
            <long></long>
            <default></default>
          </apdate>
          <apbal>
            <type>str</type>
            <long></long>
            <default></default>
          </apbal>
          <descr>
            <type>str</type>
            <long></long>
            <default></default>
          </descr>
          <balance>
            <type>int</type>
            <long></long>
            <default></default>
          </balance>
        </argument>
      </sql>

      <layout>
          <element>
            <selector>.//*[@name="aid"]</selector>
            <attribute></attribute>
            <fild>aid</fild>
          </element>
          <element>
            <selector>.//*[@name="aid"]</selector>
            <attribute>data-href</attribute>
            <fild>aid</fild>
          </element>
          <element>
            <selector>.//*[@name="aname"]</selector>
            <attribute></attribute>
            <fild>aname</fild>
          </element>
          <element>
            <selector>.//*[@name="curr"]</selector>
            <attribute></attribute>
            <fild>curr</fild>
          </element>
          <!-- <element>
            <selector>.//*[@name="apdate"]</selector>
            <attribute></attribute>
            <fild>apdate</fild>
          </element> -->
          <element>
            <selector>.//*[@name="apbal"]</selector>
            <attribute></attribute>
            <fild>apbal</fild>
          </element>
          <element>
            <selector>.//*[@name="descr"]</selector>
            <attribute></attribute>
            <fild></fild>
          </element>
          <element>
            <selector>.//*[@name="balance"]</selector>
            <attribute></attribute>
            <fild>balance</fild>
          </element>
          <element>
            <selector>.//*[@name="side"]</selector>
            <attribute></attribute>
            <fild></fild>
          </element>
          <element>
            <selector>.//*[@name="side"]</selector>
            <attribute>data-side</attribute>
            <fild>side</fild>
          </element>
          <element>
            <selector>.//*[@name="usebal"]</selector>
            <attribute></attribute>
            <fild></fild>
          </element>
          <element>
            <selector>.//*[@name="usebal"]</selector>
            <attribute>data-usebal</attribute>
            <fild>usebal</fild>
          </element>
      </layout>
    </part>
    <part></part>



  </script>
